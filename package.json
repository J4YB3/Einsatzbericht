{
  "name": "einsatzbericht",
  "author": "Jan Bechtold",
  "description": "A simple Electron application with Vue + Vuetify 3, which renders a form and connects to a database to store the form data in a table. Also renders a PDF of the form data.",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "main": "src/main/index.js",
  "scripts": {
    "dev": "vite && electron .",
    "build": "run-p type-check \"build-only {@}\" -- && electron-builder",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "core-js": "^3.37.1",
    "roboto-fontface": "*",
    "vue": "^3.4.31",
    "vuetify": "^3.6.14"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.5.1",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "npm-run-all2": "^7.0.1",
    "sass": "1.77.8",
    "sass-embedded": "^1.77.8",
    "typescript": "~5.6.3",
    "unplugin-fonts": "^1.1.1",
    "unplugin-vue-components": "^0.27.2",
    "unplugin-vue-router": "^0.10.0",
    "vite": "^5.4.10",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-vuetify": "^2.0.3",
    "vue-router": "^4.4.0",
    "vue-tsc": "^2.1.10"
  }
}
